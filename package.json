{
  "name": "starter-theme",
  "version": "1.0.0-alpha.1",
  "private": true,
  "author": "BVA",
  "description": "Custom theme for wwe-gulp",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/bvaccel-delivery/wwe"
  },
  "scripts": {
    "start": "gulp",
    "build": "gulp build",
    "build:prod": "export NODE_ENV=production && yarn build",
    "build:analyze": "export NODE_ENV=analyze && yarn build",
    "watch": "gulp watch",
    "serve": "vue-cli-service serve",
    "test": "vue-cli-service test:unit --passWithNoTests",
    "deploy": "gulp deploy",
    "lint": "vue-cli-service lint",
    "lint:scss": "stylelint 'src/**/*.scss' --syntax scss",
    "lint:scss:fix": "stylelint 'src/**/*.scss' --syntax scss --fix"
  },
  "dependencies": {
    "animated-scroll-to": "^2.0.10",
    "aos": "^3.0.0-beta.6",
    "autoprefixer": "^9.8.5",
    "core-js": "^3.6.5",
    "gsap": "^3.5.0",
    "hover.css": "^2.3.2",
    "lodash": "^4.17.19",
    "papaparse": "^5.3.0",
    "query-string": "^6.13.1",
    "swiper": "^6.1.2",
    "tailwindcss": "^1.5.2",
    "v-click-outside": "^3.0.1",
    "v-scroll-lock": "^1.3.1",
    "vanilla-lazyload": "^17.3.1",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^4.1.1",
    "vue-class-component": "^7.2.3",
    "vue-focus-lock": "^1.4.0",
    "vue-lazyload": "^1.3.3",
    "vue-mq": "^1.0.1",
    "vue-property-decorator": "^9.0.0",
    "vue2-transitions": "^0.3.0",
    "vueinview": "^1.0.7",
    "vuex": "^3.4.0",
    "whatwg-fetch": "^3.4.0"
  },
  "devDependencies": {
    "@bva/gulp-shopify-upload": "^2.5.0",
    "@types/jest": "^24.0.19",
    "@types/lodash": "^4.14.159",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli": "^4.4.6",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.4",
    "concurrently": "^5.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-ejs": "^5.1.0",
    "gulp-flatten": "^0.4.0",
    "gulp-if": "^3.0.0",
    "gulp-inject": "^5.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-svgstore": "^7.0.1",
    "gulp-watch": "^5.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss": "^7.0.32",
    "require-dir": "^1.2.0",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "tailwindcss-blend-mode": "^1.0.0",
    "typescript": "~3.9.3",
    "vinyl-named": "^1.1.0",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-stream": "^5.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "eslint . --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "stylelint --syntax scss --fix",
      "git add"
    ]
  }
}
