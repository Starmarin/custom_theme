        "type": "header",
        "content": "Mobile Menu 2"
      },
      {
        "type": "paragraph",
        "content": "Displays in the mobile menu after the main menu items."
      },
      {
        "type": "link_list",
        "id": "mobile_linklist_2",
        "label": "Mobile Explore Menu"
      }
