            "id": "linklist_raw",
            "label": "Linklist (Raw)"
          },
          {
            "type": "image_picker",
            "id": "mobile_banner_smackdown",
            "label": "Mobile Banner (Smackdown)",
            "info": "Recommended Dimenionsions: 430px x 120px"
          },
          {
            "type": "link_list",
            "id": "linklist_smackdown",
